    "#Return Job ID from submission ID using Astrometry API\n",
    "def JobID(submissionID):\n",
    "    url = 'http://nova.astrometry.net/api/submissions/'+ submissionID + '/'\n",
    "    request = requests.post(url)\n",
    "    res = str(request.json()['jobs'][0])\n",
    "    return res"
